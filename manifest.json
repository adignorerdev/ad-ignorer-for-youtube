{
    "manifest_version": 2,
    "name": "Ad-Ignore for Youtube",
    "description": "Extension for *ignoring* Youtube advertisements. Shows a fun little intermission animation while advertisements are playing.",
    "version": "0.1.173",
    "icons": {
      "128": "icon_128.png",
      "48": "icon_48.png"
    },
    "permissions": [
      "*://*.youtube.com/*"
    ],
    "content_scripts": [
        {
          "matches": ["*://*.youtube.com/*"],
          "js": ["content.js"],
          "css": ["style.css"]
        }
      ],
    "web_accessible_resources": [
      "assets/*",
      "squirrel.js"
    ]
  }